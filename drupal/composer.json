{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_toolbar": "^1.24",
        "drupal/ajax_comments": "^1.0",
        "drupal/betterlogin": "^1.2",
        "drupal/block_field": "^1.0",
        "drupal/console": "^1.8",
        "drupal/core": "~8.6.1",
        "drupal/ctools": "^3.0",
        "drupal/devel": "^2.0",
        "drupal/draggableviews": "^1.2",
        "drupal/easy_breadcrumb": "^1.8",
        "drupal/entity_reference_revisions": "^1.5",
        "drupal/feeds": "^3.0@alpha",
        "drupal/field_group": "^3.0",
        "drupal/homebox": "1.x-dev",
        "drupal/multiple_fields_remove_button": "^1.0",
        "drupal/paragraphs": "^1.3",
        "drupal/pathauto": "^1.3",
        "drupal/require_login": "^2.0",
        "drupal/restui": "^1.15",
        "drupal/search_autocomplete": "^1.1",
        "drupal/select2": "^1.1",
        "drupal/smtp": "^1.0",
        "drupal/viewfield": "3.x-dev",
        "drupal/views_autocomplete_filters": "^1.2",
        "drupal/views_bulk_edit": "^2.2",
        "drupal/views_data_export": "^1.0",
        "drupal/views_entity_form_field": "^1.0",
        "drupal/views_field_formatter": "^1.8",
        "drush/drush": "~9.1dev",
        "npm-asset/qtip2": "^3.0",
        "npm-asset/select2": "^4.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "installer-types": ["bower-asset", "npm-asset"],
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
        "patches": {
          "drupal/core": {
            "Entity Reference Views 2174633": "https://www.drupal.org/files/issues/2018-12-12/2174633-227.patch",
            "Firewall stripping ajax header 2580191": "https://www.drupal.org/files/issues/ajax-remove-X-Drupal-Ajax-Token-verification-2580191-33.patch"
          },
          "drupal/draggableviews": {
            "Fix draggable views grouping": "https://www.drupal.org/files/issues/2018-09-07/not_working_with_group_by-draggableviews-2867348-26.patch"
          },
          "drupal/homebox": {
            "Redirect Access Denied to User Login Page": "patches/homebox_redirect_access_denied_response.patch"
          }
        }
    }
}
